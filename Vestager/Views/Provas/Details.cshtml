@model Vestager.Domain.Entities.Prova

@{
    ViewData["Title"] = "Details";
}

<h2>Detalhes da Prova</h2>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Cliente)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Cliente.UserID)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DataProva)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DataProva)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-warning" asp-action="Edit" asp-route-id="@Model.ProvaID">Editar</a> 
    <a class="btn btn-primary" asp-action="Index">Voltar</a>
</div>
