@model Vestager.Domain.Entities.Ajuste

@{
    ViewData["Title"] = "Details";
}

<h2>Detalhes do Ajuste</h2>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nome)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nome)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nivel)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nivel)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.TempoEstimado)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TempoEstimado)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-warning" asp-action="Edit" asp-route-id="@Model.AjusteID">Editar</a> 
    <a class="btn btn-primary" asp-action="Index">Voltar</a>
</div>
